{
	"name": "monkeyteam-io",
	"description": "monkeyteam.io - ChatGPT Features For Vs-Code",
	"version": "0.0.1",
	"publisher": "monkeyteam.io",
	"private": true,
	"icon": "./media/icon.png",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "monkeyteam-io",
			"properties": {
				"API_KEY": {
					"type": "string"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "ChatGPT-GPTView",
					"title": "monkeyteam-io Chat",
					"icon": "media/icon.png"
				}
			]
		},
		"commands": [
			{
				"command": "code-instance.explain",
				"category": "code-instance",
				"title": "monkeyteam.io -> Explain Code"
			},
			{
				"command": "code-instance.unit_test_case",
				"category": "code-instance",
				"title": "monkeyteam.io -> Generate Unit Test Case"
			},
			{
				"command": "code-instance.add_comments",
				"category": "code-instance",
				"title": "monkeyteam.io -> Add Comments"
			},
			{
				"command": "code-instance.find_bugs",
				"category": "code-instance",
				"title": "monkeyteam.io -> Find Bugs"
			},
			{
				"command": "code-instance.code_refactor",
				"category": "code-instance",
				"title": "monkeyteam.io -> Refactor Code"
			},
			{
				"command": "code-instance.code_optimize",
				"category": "code-instance",
				"title": "monkeyteam.io -> Optimize Code"
			},
			{
				"command": "code-instance.ask_anything",
				"category": "code-instance",
				"title": "monkeyteam.io -> Ask Anything"
			}
		],
		"menus": {
			"view/title": [],
			"editor/context": [
				{
					"when": "editorTextFocus",
					"command": "code-instance.explain",
					"group": "code-instance"
				},
				{
					"when": "editorTextFocus",
					"command": "code-instance.unit_test_case",
					"group": "code-instance"
				},
				{
					"when": "editorTextFocus",
					"command": "code-instance.add_comments",
					"group": "code-instance"
				},
				{
					"when": "editorTextFocus",
					"command": "code-instance.find_bugs",
					"group": "code-instance"
				},
				{
					"when": "editorTextFocus",
					"command": "code-instance.code_refactor",
					"group": "code-instance"
				},
				{
					"when": "editorTextFocus",
					"command": "code-instance.code_optimize",
					"group": "code-instance"
				},
				{
					"when": "editorTextFocus",
					"command": "code-instance.ask_anything",
					"group": "code-instance"
				}
			]
		},
		"keybindings": [
			{
				"command": "code-instance.explain",
				"key": "ctrl+e",
				"mac": "cmnd+e"
			},
			{
				"command": "code-instance.unit_test_case",
				"key": "ctrl+u",
				"mac": "cmnd+u"
			},
			{
				"command": "code-instance.add_comments",
				"key": "ctrl+k",
				"mac": "cmnd+k"
			},
			{
				"command": "code-instance.find_bugs",
				"key": "ctrl+m",
				"mac": "cmnd+m"
			},
			{
				"command": "code-instance.code_refactor",
				"key": "ctrl+o",
				"mac": "cmnd+o"
			},
			{
				"command": "code-instance.code_optimize",
				"key": "alt+a",
				"mac": "alt+a"
			},
			{
				"command": "code-instance.ask_anything",
				"key": "alt+k",
				"mac": "alt+k"
			}
		],
		"views": {
			"ChatGPT-GPTView": [
				{
					"id": "chatGPT.gptview",
					"name": "monkeyteam.io Chat",
					"type": "webview"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"build": "vsce package",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"@vscode/vsce": "^2.24.0",
		"eslint": "^8.32.0",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"axios": "^1.6.8",
		"getmac": "^6.6.0"
	}
}
